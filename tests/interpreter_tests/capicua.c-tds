class capicua{
    int length;
    int num[5];
    
    boolean isCapicua(){
        int i;
        for i=0,length/2 {
            if(num[i] != num[(length-1) -i]){
                return false;
            }
        }
        return true;
    }
    
    void main(){
        length = 5;
        num[0] = 1;
        num[1] = 0;
        num[2] = 7;
        num[3] = 0;
        num[4] = 1;        
        print isCapicua();
        
        num[0] = 2;
        num[1] = 1;
        num[2] = 7;
        num[3] = 1;
        num[4] = 1;        
        print isCapicua();
        return;
        
    
    }

}
