class pp{

int factorial(int n){

	int i = n;  // no permite inicializar en las declaraciones
	int total = 1; // no permite inicializar en las declaraciones
 
	while(i > 0){

		total += total * + i; // operadores consecutivos * +
		i = i - 1;

	}
	return total;

} // end method factorial

} // end class pp
