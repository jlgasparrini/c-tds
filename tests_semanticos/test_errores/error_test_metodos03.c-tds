class test_metodos{
   float c;

   void alo(boolean b, int x){
      x= x + 1;
      return ; 
   }

   int alo2(int x){
      alo(true, x);
      x=  x + 1; 
      return x; 
   }
 
   void main(){
        int y;
        y = 7;
        c = y *8;
        y = alo2(y, 5, c); // Error alo2 tiene un unico parametro
        y = alo2(-5.3); // Error alo2 toma un entero
        alo((c+3)*y, false); // Error el orden de los tipos de los parametros esta mal
        return;
   }

}
