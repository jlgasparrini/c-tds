class test_float{

float potencia(float x, int n){

	int i; 
	boolean neg;
	float prod;
        neg = false;
        i = 1;
        prod = 1.0;

	if (n < 0){
		n = -n;
		neg = !neg;
	}

	while(i <= n){
		prod = prod * x;
		i = i + 1;
	}
	if (!(!neg)){
		return 1.0/prod;
        }
	else{
		return prod;
        }
	return -100.1;
}

float sumatoria(float b){
	float total;
	float i;
	float res;
        total = 0.0;
        i = b;
        
	while(i > 0  || i > 0 )   {
		if(i > 0){
			total = total + i;
                }
		i -= 1;
	} /* fin while */
	res = total;
	return res;

}

